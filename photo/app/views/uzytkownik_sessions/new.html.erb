<% title "Zaloguj się" %>

<% form_for @uzytkownik_session do |form| %>
<%= form.error_messages %>
<ol class="formList">
  <li>
    <%= form.label :login, "Login" %>
    <%= form.text_field :login %>
  </li>
  <li>
    <%= form.label :password, "Hasło" %>
    <%= form.password_field :password %>
  </li>
  <li>
    <%= form.submit "Zaloguj" %>
  </li>
</ol>

<% end %>
